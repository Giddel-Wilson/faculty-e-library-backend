{
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "postinstall": "node -e \"if(process.env.INSTALL_ROLLUP_OPTIONAL==='true'){require('./scripts/install-rollup-optional.cjs')} else {console.log('Skipping install-rollup-optional (INSTALL_ROLLUP_OPTIONAL!=true)')}\"",
    "vercel-build": "npm run build",
    "lint": "eslint \"src\"",
    "start": "dotenv -c '.env' -- node dist/app",
    "start:server": "node dist/server.js",
    "dev": "tsx watch ./src/app.ts"
  },
  "dependencies": {
    "@rollup/rollup-linux-x64-gnu": "file:rollup-shims/rollup-linux-x64-gnu",
    "@rollup/wasm-node": "^3.0.0",
    "@adminjs/express": "^6.1.0",
    "@adminjs/mongoose": "^4.1.0",
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/lib-storage": "^3.758.0",
    "@types/cors": "^2.8.17",
    "adminjs": "^7.7.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.12.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "serverless-http": "^3.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.15.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.1.0",
    "tslib": "^2.5.0",
    "tsx": "^4.19.3",
    "typescript": "^5.0.4"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64": "*",
    "@rollup/rollup-linux-x64-gnu": "npm:null@*",
    "@rollup/rollup-linux-x64-musl": "*"
  },
  "name": "faculty-of-science-material-upload-platform"
}
